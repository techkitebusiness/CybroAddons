                                            <i class="material-icons">&#xe315;</i>
                                            <li class="breadcrumb-item"><a href="/shop/cart">Cart</a></li>
                                            <i class="material-icons">&#xe315;</i>
                                            <li class="breadcrumb-item active" aria-current="page">Payment</li>
                                        </ol>
                                    </nav>
                                </div>
                            </div>
                        </div>
                    </div>
