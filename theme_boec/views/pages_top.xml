<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <template id="boec_login" inherit_id="website.login_layout" name="Boec Login Top">
            <xpath expr="//div[hasclass('oe_website_login_container')]" position="before">
                <Section class="banner_sub">
                    <div class="wrapper">
                        <div class="container">
                            <div class="row">
                                <div class="sub_page">
                                    <div class="name">
                                        Login
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </Section>
            </xpath>
        </template>

        <template id="boec_wishlist_top" inherit_id="website_sale_wishlist.product_wishlist" name="Boec WishList Top">
            <xpath expr="//div[hasclass('js_sale')]" position="before">
                <Section class="banner_sub">
                    <div class="wrapper">
                        <div class="container">
                            <div class="row">
                                <div class="sub_page">
                                    <div class="name">
                                        Wishlist
                                    </div>
                                    <nav aria-label="breadcrumb " class="brd">
                                        <ol class="breadcrumb">
                                            <li class="breadcrumb-item"><a href="/">Home</a></li>
                                            <i class="material-icons">&#xe315;</i>
                                            <li class="breadcrumb-item"><a href="/shop">Shop</a></li>
                                            <i class="material-icons">&#xe315;</i>
                                            <li class="breadcrumb-item active" aria-current="page">Wishlist</li>
                                        </ol>
                                    </nav>
                                </div>
                            </div>
                        </div>
                    </div>
                </Section>
            </xpath>
        </template>

        <template id="boec_comparison_top" inherit_id="website_sale_comparison.product_compare" name="Boec Comparison Top">
