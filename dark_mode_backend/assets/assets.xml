<odoo>
    <data>
        <template id="dark_mode_backend" name="Code Backend Theme Assets" inherit_id="web.assets_backend">
            <xpath expr=".">
                <link rel="stylesheet" href="/dark_mode_backend/static/src/scss/theme_accent.scss"/>
                <link rel="stylesheet" href="/dark_mode_backend/static/src/scss/navigation_bar.scss"/>
                <link rel="stylesheet" href="/dark_mode_backend/static/src/scss/datetimepicker.scss"/>
                <link rel="stylesheet" href="/dark_mode_backend/static/src/scss/theme.scss"/>
                <link rel="stylesheet" href="/dark_mode_backend/static/src/scss/sidebar.scss"/>
                <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@400;600&amp;display=swap" rel="stylesheet"/>
            </xpath>
        </template>
        <template id="dark_mode_backend_assets_frontend" inherit_id="web.assets_frontend">
            <xpath expr="//link[last()]" position="after">
                <link rel="stylesheet" href="/dark_mode_backend/static/src/scss/login.scss"/>
                <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@400;600&amp;display=swap" rel="stylesheet"/>
            </xpath>
        </template>
    </data>
</odoo>
